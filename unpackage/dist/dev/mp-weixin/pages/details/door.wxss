.door {
  overflow: hidden;
}
.door .title {
  height: 50px;
  line-height: 50px;
  color: #6147f4;
  text-align: center;
  letter-spacing: 5px;
  background-color: #fff;
  margin: 7rpx 0;
}
.door .title1 {
  height: 50px;
  line-height: 50px;
  color: black;
  text-align: center;
  background-color: #fff;
  margin: 7rpx 0;
  font-size: 50rpx;
  border-bottom-style: solid;
  border-bottom-width: 0.5px;
}
.door .nav {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.door .nav .nav_item {
  width: 50%;
  text-align: center;
}
.door .nav .nav_item view {
  width: 120rpx;
  height: 120rpx;
  border-radius: 60rpx;
  margin: 10px auto;
  line-height: 120rpx;
  color: #6147f4;
  font-size: 80rpx;
}
.door .nav .nav_item .aa {
  font-size: 120rpx;
}
.door .nav .nav_item text {
  font-size: 30rpx;
}
.door .nav .nav_item1 {
  width: 50%;
  text-align: center;
}
