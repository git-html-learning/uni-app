.tem {
  overflow: hidden;
}
.tem .title {
  height: 50px;
  line-height: 50px;
  color: #6147f4;
  text-align: center;
  letter-spacing: 5px;
  background-color: #fff;
  margin: 7rpx 0;
}
.tem .title1 {
  height: 50px;
  line-height: 50px;
  color: black;
  text-align: center;
  background-color: #fff;
  margin: 7rpx 0;
  font-size: 50rpx;
  border-bottom-style: solid;
  border-bottom-width: 0.5px;
}
.tem .charts-box {
  width: 100%;
  height: 300px;
}
.tem .layout {
  background-color: #fff;
  position: relative;
  overflow: hidden;
  margin-top: 2px;
  margin-left: 2%;
  width: 96%;
  height: 550px;
  border: 1px solid black;
  border-radius: 20px;
}
.tem .layout1 {
  background-color: #fff;
  position: relative;
  overflow: hidden;
  margin-top: 2px;
  margin-left: 4%;
  width: 350px;
  height: 50px;
  border: 1px solid black;
  border-radius: 20px;
}
.tem .layout .pic {
  width: 350px;
  height: 130px;
  /* border: 1px solid black;            */
}
.tem .left-item {
  width: 40%;
  height: 100%;
  float: left;
  /* border: 1px solid black;  */
}
.tem .right-item {
  width: 40%;
  height: 100%;
  float: right;
  /* border: 1px solid black;  */
}
.tem ._table {
  border-right: 1px solid #e8ebf3;
  border-top: 1px solid #e8ebf3;
  border-collapse: collapse;
  margin-top: 35px;
  float: left;
}
.tem .left-item ._table {
  margin-left: 10px;
}
.tem .right-item ._table {
  float: right;
  margin-right: 10px;
}
.tem ._td {
  border-left: 1px solid #e8ebf3;
  border-bottom: 1px solid #e8ebf3;
  font-size: 14px;
  padding: 2px 3px;
}
.tem .left-num {
  float: left;
  width: 10%;
  height: 100%;
}
.tem .right-num {
  float: left;
  width: 10%;
  height: 100%;
}
.tem .first {
  margin-top: 30px;
  width: 100%;
  height: 58px;
  border-bottom: 1px solid black;
  border-right: 1px solid black;
}
.tem .second {
  width: 100%;
  height: 130px;
  /* background-color: rgb(23, 39, 79);    */
  border-bottom: 1px solid black;
  border-right: 1px solid black;
}
.tem .third {
  width: 100%;
  height: 125px;
  /* background-color: rgb(69, 141, 51); */
  border-bottom: 1px solid black;
  border-right: 1px solid black;
}
.tem .right-num .first {
  border-right: none;
}
.tem .right-num .second {
  border-right: none;
}
.tem .right-num .third {
  border-right: none;
}
.tem .left-num ._table {
  margin-top: 50px;
  margin-left: 1px;
  width: 47px;
}
.tem .right-num ._table {
  margin-right: 12px;
  width: 47px;
}
.tem .left-num ._tr ._td {
  border-bottom: 1px solid;
}
.error {
  color: red;
}

