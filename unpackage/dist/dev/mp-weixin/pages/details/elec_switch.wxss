.elec {
  overflow: hidden;
}
.elec .title {
  height: 50px;
  line-height: 50px;
  color: black;
  text-align: center;
  background-color: #fff;
  margin: 7rpx 0;
  font-size: 50rpx;
  border-bottom-style: solid;
  border-bottom-width: 0.5px;
}
.elec .nav {
  display: flex;
}
.elec .nav .nav_item {
  width: 50%;
  text-align: center;
  margin-top: 20px;
}
.elec .nav .nav_item .aa {
  width: 120rpx;
  height: 120rpx;
  margin: 10px auto;
  line-height: 120rpx;
  font-size: 80rpx;
  color: #515151;
}
.elec .nav .nav_item .bb {
  width: 120rpx;
  height: 120rpx;
  margin: 10px auto;
  line-height: 120rpx;
  font-size: 80rpx;
  color: #6147f4;
}
.elec .nav .nav_item text {
  font-size: 30rpx;
}
.elec .nav .nav_item1 {
  width: 100%;
  text-align: center;
  margin-top: 20px;
}
.elec .nav .nav_item1 .aa {
  width: 120rpx;
  height: 120rpx;
  margin: 10px auto;
  line-height: 120rpx;
  font-size: 80rpx;
  color: #515151;
}
.elec .nav .nav_item1 .bb {
