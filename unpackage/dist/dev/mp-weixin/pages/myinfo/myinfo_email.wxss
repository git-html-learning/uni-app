page {
  background-color: #fff;
}
.content {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.content .title {
  height: 300rpx;
  font-size: 35rpx;
  font-weight: 600;
  display: flex;
  align-items: center;
  justify-content: center;
}
.content .data {
  width: 500rpx;
  height: 90rpx;
  display: flex;
  align-items: center;
  border-top: solid 1rpx #eee;
  border-bottom: solid 1rpx #eee;
}
.content .data .data_title {
  margin-right: 50rpx;
}
.content .data input {
  font-size: 28rpx;
}
.content .operation {
  margin-top: 600rpx;
}
.content .operation button {
  width: 400rpx;
}

