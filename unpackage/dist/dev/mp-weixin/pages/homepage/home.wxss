.home .userinfo {
  display: flex;
  margin: 5px 0;
  padding: 10px;
  border-radius: 5px;
  background-color: #FFFFFF;
  align-items: center;
  justify-content: space-between;
  box-shadow: 0 0 5px #c8c7cc;
}
.home swiper {
  width: 750rpx;
  height: 380rpx;
}
.home swiper image {
  height: 100%;
  width: 100%;
}
.home .nav {
  display: flex;
}
.home .nav .nav_item {
  width: 50%;
  text-align: center;
}
.home .nav .nav_item view {
  width: 120rpx;
  height: 120rpx;
  border-radius: 60rpx;
  margin: 10px auto;
  line-height: 120rpx;
  color: #6147f4;
  font-size: 80rpx;
}
.home .nav .nav_item .aa {
  font-size: 120rpx;
}
.home .nav .nav_item text {
  font-size: 30rpx;
}
